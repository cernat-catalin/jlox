assert((1 == 1 ? 10 : 11) == 10, "Ternary operator wrong result!");
assert((1 == 2 ? 10 : 11) == 11, "Ternary operator wrong result!");

assert((1 == 2 ? 4 == 5 ? 10 : 11 : 12) == 12, "Ternary operator wrong result!");
assert((1 == 1 ? 4 == 5 ? 10 : 11 : 12) == 11, "Ternary operator wrong result!");
assert((1 == 1 ? 4 == 4 ? 10 : 11 : 12) == 10, "Ternary operator wrong result!");
